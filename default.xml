<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- This is the list of repos we're nabbing things from here... -->
  <remote  name="github"
           fetch="http://github.com" />

  <remote  name="yocto"
	   fetch="http://git.yoctoproject.org" />

  <remote  name="openembedded"
     	   fetch="git://git.openembedded.org/" />

  <!-- Unless overridden by the project line-item, we're grabbing from
       the head of the master branch for any given git repo for this
       project... -->
  <default revision="master"/>

 <!-- Yocto/Poky core pieces, including .NET support -->
 <project name="git/poky" remote="yocto" revision="honister" path="." />
 <project name="meta-openembedded" remote="openembedded" revision="honister" path="meta-openembedded" />

 <!-- The meta-clang layer for the optional use of clang/LLVM with things in your build... -->
 <project name="kraj/meta-clang" remote="github" revision="honister" path="meta-clang" />

 <!-- The ROS 1/2 metadata layer... -->
 <project name="ros/meta-ros" remote="github" revision="honister" path="meta-ros" />

 <!-- The runit metadata layer, one of the lynchpin pieces of PHA Linux... -->
 <project name="madscientist42/meta-runit" remote="github" path="meta-runit" />

 <!-- Pull in the Xilinx tools stuff...  We're going to approximate petalinux as a pure Yocto solution...  -->
 <project name="Xilinx/meta-xilinx-tools" remote="github" revision="rel-v2022.2" path="meta-xilinx-tools" />
 <project name="Xilinx/meta-xilinx" remote="github" revision="rel-v2022.2" path="meta-xilinx" />
 <project name="Xilinx/meta-petalinux" remote="github" revision="rel-v2022.2" path="meta-petalinux" />
 <project name="Xilinx/meta-som" remote="github" revision="rel-v2022.2" path="meta-som" />
 <project name="Xilinx/meta-virtualization" remote="github" revision="rel-v2022.2" path="meta-virtualization" />
 <project name="Xilinx/meta-jupyter" remote="github" revision="rel-v2022.2" path="meta-jupyter" />
 <project name="Xilinx/meta-openamp" remote="github" revision="rel-v2022.2" path="meta-openamp" />
 <project name="Xilinx/meta-security" remote="github" revision="rel-v2022.2" path="meta-security" />
 <project name="Xilinx/meta-qt5" remote="github" revision="rel-v2022.2" path="meta-qt5" />
 <project name="Xilinx/meta-python2" remote="github" revision="rel-v2022.2" path="meta-python2" />
 <project name="Xilinx/meta-browser" remote="github" revision="rel-v2022.2" path="meta-browser" />

 <!-- Pull in an OE metadata layer from PHA Technologies...the BSP and enhancements for the NanoPI Neo2/Neo-Plus2 boards -->
 <project name="madscientist42/meta-pha" remote="github" path="meta-pha"/>

 <!-- Pull in our build config... -->
 <project name="madscientist42/oe_build_config" remote="github" path="build"/>

</manifest>


